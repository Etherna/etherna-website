---
import Layout from "../layouts/Layout.astro"

export function getStaticPaths() {
  return [
    { params: { path: "/" } },
    { params: { path: "" } },
    { params: { path: "it" } },
    { params: { path: "contacts" } },
    { params: { path: "it/contatti" } },
  ]
}
---

<Layout>
  <pre>
    {JSON.stringify(Astro.params, null, 2)}
  </pre>
</Layout>
