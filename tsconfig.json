{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "exactOptionalPropertyTypes": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "typeRoots": ["src/typings/"]
  }
}
