---
import { StaticPage } from "@/components/layout/static-page"
import { BrandKit } from "@/components/site/brand-kit"

import type { ViewProps } from "@/queries/fetch-props"

interface Props extends ViewProps<"brand-kit"> {}

const { title, data } = Astro.props
---

<StaticPage title={title}>
  <BrandKit
    colors={data.brand.colors}
    fonts={data.brand.fonts}
    logos={data.brand.logos}
    client:load
  />
</StaticPage>
