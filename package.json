{
  "name": "etherna-website",
  "version": "2.0.0",
  "author": "Mattia Dalzocchio",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/Etherna/etherna-website"
  },
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "snapshot": "turbo run snapshot",
    "migrate": "turbo run migrate",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "find . \\( -name 'node_modules' -o -name '.turbo' -o -name '.next' -o -name 'dist' \\)  -type d -prune -exec rm -rf '{}' +",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lexical": "0.18.0",
    "lucide-react": "0.452.0",
    "payload": "3.0.0-beta.116",
    "tailwind-merge": "2.5.3",
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@swc/core": "1.7.35",
    "@tailwindcss/typography": "0.5.15",
    "@types/node": "22.7.5",
    "autoprefixer": "10.4.20",
    "eslint": "9.12.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.13",
    "tailwindcss-animate": "1.0.7",
    "tsconfig": "workspace:*",
    "tsup": "8.3.0",
    "turbo": "2.1.3",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=22"
  }
}
