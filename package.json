{
  "name": "etherna-website",
  "version": "2.0.0",
  "author": "Mattia Dalzocchio",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Etherna/etherna-website"
  },
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "migrate": "cd ./apps/cms && pnpm migrate-up",
    "bootstrap": "cd ./apps/cms && pnpm bootstrap && pnpm migrate-up"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.10.2",
    "eslint": "8.55.0",
    "prettier": "3.1.0",
    "tsconfig": "workspace:*",
    "turbo": "1.11.3",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "tslib": "2.6.2"
  }
}
