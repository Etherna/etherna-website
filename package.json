{
  "name": "etherna-website",
  "version": "2.0.0",
  "author": "Mattia Dalzocchio",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Etherna/etherna-website"
  },
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "migrate": "cd ./apps/cms && pnpm migrate-up",
    "bootstrap": "cd ./apps/cms && pnpm bootstrap && pnpm migrate-up"
  },
  "dependencies": {
    "@mattiaz9/slate-jsx": "0.2.2",
    "slate": "0.101.5",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.11.5",
    "eslint": "8.56.0",
    "prettier": "3.2.4",
    "tsconfig": "workspace:*",
    "turbo": "1.11.3",
    "typescript": "5.3.3"
  }
}
